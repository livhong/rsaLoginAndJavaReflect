<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ParseCryptogram</servlet-name>
    <servlet-class>com.livhong.action.ParseCryptogram</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RSALogin</servlet-name>
    <servlet-class>com.livhong.action.RSALogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.livhong.action.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestFormObject</servlet-name>
    <servlet-class>com.livhong.action.TestFormObject</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>ParseCryptogram</servlet-name>
    <url-pattern>/parse</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RSALogin</servlet-name>
    <url-pattern>/loginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestFormObject</servlet-name>
    <url-pattern>/testFormObject</url-pattern>
  </servlet-mapping>

</web-app>